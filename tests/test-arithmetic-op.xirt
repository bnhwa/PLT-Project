num main() {
    num k;

    k = 1 + 9;
    printn(k);

    k = 1 + -9;
    printn(k);

    k = 9 + (-4);
    printn(k);

    k = -8 + 2;
    printn(k);

     k = 9 - 4;
    printn(k);

    k = 0 - 0;
    printn(k);

    k = 3 - 4;
    printn(k);

    k = 2 - 2;
    printn(k); 

    k = 8 * 2;
    printn(k);

    k = -8 * 2;
    printn(k);

     k = 8.3 * 2;
    printn(k);
    
    k = 2 * 0;
    printn(k);

    
    k = -1.3 * 0.9;
    printn(k);

    k = 1.3 * -0.9;
    printn(k);

    k = 1 / 1;
    printn(k);

    k = 1 / 0;
    printn(k);

    k = 1/ 7;
    printn(k);

    k = -1 / -7;
    printn(k);

    k = 1 % 1;
    printn(k);

    k = 9.3 % 6;
    printn(k);

    k = 9.3 % 1.4;
    printn(k);

    k = 10 % 2;
    printn(k);

    k = 7 % 3;
    printn(k);

    /* mod does not work with negatives */
}